<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC
  "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<schema-selection match-schema="public" />

	<table-filter match-schema="public" match-name="database.*" exclude="true" />
	<table-filter match-schema="public" match-name="qrtz.*" exclude="true" />
	<table-filter match-schema="public" match-name="revinfo" exclude="true" />
	<table-filter match-schema="public" match-name="usuario_aud" exclude="true" />
	<table-filter match-schema="public" match-name="oauth_.*" exclude="true" />
	<table-filter match-schema="public" match-name="clientdetails" exclude="true" />
	
</hibernate-reverse-engineering>